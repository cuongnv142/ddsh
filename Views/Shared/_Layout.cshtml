<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
@*     <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests" />
 *@    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <link rel="icon" href="/images/logo.png">

    <link rel="stylesheet" type="text/css" href="/styles/slick.css">
    <link rel="stylesheet" type="text/css" href="/styles/slick-theme.css">
    <script src="/js/slick.min.js"></script>

    <script type="text/javascript" src="/js/jquery.easing.1.3.js"></script>
    <link rel="stylesheet" type="text/css" href="/styles/basicStyle.css">
    <link rel="stylesheet" type="text/css" href="/lib/toastr/css/toastr.min.css">
    <link rel="stylesheet" type="text/css" href="/styles/responsiveStyle.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">
    <script src="/js/jquery.dropdown.js"></script>
    <script src="/js/myScript.js"></script>
    <!-- Magnific Popup core JS file -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="/lib/image-popup/magnific-popup.css">
</head>
<style>
    
   
     a{
        text-decoration:none
    }
    #header .contact .custom-select1 {
        height: 20px;
        background-repeat: no-repeat;
        background-position: right 9px;
        position: relative;
        width: 390px;
        top: 2px;
        color: #ffc107;
        font-size: 20px;
        text-align: center;
    }
    .logo_name{
        display: flex;
        justify-content: space-between;
    }

    .contact {
        display:flex;
        align-items:center;
        justify-content:center
    }

    .rightContact {
        display: flex;
    }

    select {
        -webkit-appearance: auto !important;
    }

</style>
<style>
    .toast-error {
        background-color: #BD362F !important;
        font-size: 1rem !important;
    }

    .toast-info {
        background-color: #2F96B4 !important;
        font-size: 1rem !important;
    }
</style>
<body>
    <div id="container">
        <div id="header" style="height:auto;">
            <div class="maincontent" style="max-width:1220px;">
                <div class="contact row">
                    <div class="logo_name col-lg-7 col-md-6 col-12" style="justify-content:unset;">
                        <div class="logo">
                            <a asp-action="Index" asp-controller="Home"><img src="/images/logo.png" width="70" height="70" alt=""></a>
                        </div>
                        <div class="lang custom-select1" style="width:630px !important; overflow:hidden;height:auto;">
                            PHẦN MỀM CƠ SỞ DỮ LIỆU VỀ CÁC HỆ SINH THÁI CÓ ĐA DẠNG SINH HỌC CAO TRONG PHẠM VI RỪNG PHÒNG HỘ
                        </div>
                    </div>
                    <ul class="rightContact col-lg-5 col-md-6 col-12" style="float:unset; align-content:center; margin:0;">
                        <li>
                            <i class="icon phone"></i>
                            <span>
                                Hotline<span>
                                    0243.8438792
                                </span>
                            </span>
                        </li>
                        @* <li>
                        <i class="icon email"></i>
                        <span>Email<span>info@vnuforest.com.vn</span></span>
                        </li> *@
                        <li>
                            <i class="icon map"></i>
                            <span>Địa chỉ<span>Cục Lâm nghiệp - Bộ NN và PTNT, số 2 Ngọc Hà, Ba Đình, Hà Nội.</span></span>
                        </li>
                    </ul>
                </div><!--end contact-->

                <div class="header2">
                    @*  <div class="logo"><a href="javascript:;"><img src="/images/logo.png" width="120" height="61" alt="" /></a></div> *@
                    <div class="nav">
                        <a href="javascript:;" class="navBtn" ><span></span><span></span><span></span></a>
                        <ul >
                            <li><a asp-action="Index" asp-controller="Home" id="homeBtn" class="active"><span>Trang chủ</span>&nbsp;</a></li>
                            <li><a asp-action="introduce" asp-controller="Home" >Giới thiệu</a></li>
                            <li><a asp-action="Address" asp-controller="Home">Bản đồ</a></li>
                            <li><a asp-action="Index" asp-controller="Natural" asp-route-IdLoai ="1">CSDL Động vật</a></li>
                            <li><a asp-action="Index" asp-controller="Natural" asp-route-IdLoai="0">CSDL Thực vật</a></li>
                            <li><a asp-action="News" asp-controller="Home">Tin tức</a></li>
                            <li><a asp-action="Tour" asp-controller="Home">Du lịch</a></li>
                            <li><a asp-action="Contact" asp-controller="Home">Liên hệ</a></li>
                            <li><a href="/admin/Home/Index">Đăng nhập</a></li>
                        </ul>
                    </div><!--end nav-->
                    <div style="clear:both;"></div>
                </div><!--header2-->
            </div><!--end maincontent-->
        </div><!--end header-->
        
        <div id="content">
            @RenderBody()
        </div><!--end content-->

        <div id="footer">
            <div class="maincontent">
                
                <div class="info" style="width:25%;">
                    <div class="logo"><a href="javascript:;"><img src="/images/logo.png" width="180" height="180" alt="" /></a></div>
                </div><!--end info-->
                <div class="subLink" style="width:70%;">
                    <ul style="width:30% !important;">
                        <li><a asp-action="introduce" asp-controller="Home" >Giới thiệu</a></li>
                        <li><a asp-action="Index" asp-controller="Natural" asp-route-IdLoai="1">CSDL Động vật</a></li>
                        <li><a asp-action="Index" asp-controller="Natural" asp-route-IdLoai="0">CSDL Thực vật</a></li>
                        <li><a asp-action="News" asp-controller="Home">Tin tức</a></li>
                        <li><a asp-action="Contact" asp-controller="Home">Liên hệ</a></li>
                    </ul>
                    <ul style="color:white;width:70% !important;line-height: 40px;">
                        <li>PHẦN MỀM CƠ SỞ DỮ LIỆU VỀ CÁC HỆ SINH THÁI CÓ ĐA DẠNG SINH HỌC CAO TRONG PHẠM VI RỪNG PHÒNG HỘ</li>
                        <li>Địa chỉ: Cục Lâm nghiệp-Bộ NN và PTNT, số 2 Ngọc Hà, Ba Đình, Hà Nội.</li>
                        <li>Hotline: 0243.8438792</li>
                        <li>Email: tonghop@vnforest.gov.vn</li>
                    </ul>
                </div><!--end sublink-->
                <div style="clear:both;"></div>
                <p class="copyright">VNUFOREST © 2021 Allright Reserved.</p>
            </div><!--end maincontent-->
        </div><!--end footer-->
    </div><!--end container-->
    <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#selectNationPark" style="    position: fixed;
    bottom: 20px;
    right: 20px;
    height: 60px;
    width: 60px;
    text-align: center;
    opacity:0.5;
    z-index:99999;
    color: white;
    font-size: 32px;
    border-radius: 50%;
    background-color: #555;
    cursor: pointer;
    outline:none;
">
        <i class="fa-solid fa-gears" style="color: #ffffff;"></i>
    </button>
    <div class="modal fade" id="selectNationPark" tabindex="-1" aria-labelledby="selectNationPark" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Chọn giao diện</h5>
                </div>
                <div class="modal-body">
                    <p>Vui lòng chọn giao diện</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary form-control" onclick="saveSession('All')">Trang chính</button>
                    <button type="button" class="btn btn-primary form-control" onclick="saveSession('NationParkNamGiang')">BQL RPH Nam Giang</button>
                    <button type="button" class="btn btn-primary form-control" onclick="saveSession('NationParkMuongTe')">BQL RPH Mường Tè</button>
                </div>
            </div>
        </div>
    </div>

    <script>
        $.ajax({
            url: '/Home/GetSessionValue', // Đường dẫn đến action xử lý yêu cầu ở phía server
            method: 'GET',
            success: function (result) {
                if (!result) {
                    $(document).ready(function () {
                        $('#selectNationPark').modal('show');
                    });
                }
            },
            error: function () {
                console.error('Lỗi khi lấy giá trị từ session.');
            }
        });

        function saveSession(value) {
            // Sử dụng AJAX để gửi request lưu session
            $.ajax({
                url: '/Home/SaveSession', // Thay đổi đường dẫn tùy theo tên controller và action của bạn
                method: 'POST',
                data: { value: value },
                success: function (result) {
                    var currentUrl = window.location.href;
                    // // Thiết lập phần query parameters thành chuỗi trống
                    var updatedUrl = currentUrl.split('?')[0];
                    window.history.replaceState({}, document.title, updatedUrl);
                    window.location.reload()
                },
                error: function () {
                    alert('Error saving session!');
                }
            });
        }
    </script>
    <script src="/lib/image-popup/jquery.magnific-popup.min.js"></script>
    <script src="/lib/toastr/js/toastr.init.js"></script>
    <script src="/lib/toastr/js/toastr.min.js"></script>
    @RenderSection("CilentScript", required:false)
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</html>
